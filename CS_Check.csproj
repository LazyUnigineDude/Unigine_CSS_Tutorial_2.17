	  <None Include="data\Scripts\Camera\ThirdPersonCameraProperty.prop.meta" />
	  <None Include="data\Scripts\Character\Animation\AnimationController.cs.meta" />
	  <None Include="data\Scripts\Character\Animation\AnimationProperty.prop" />
	  <None Include="data\Scripts\Character\Animation\AnimationProperty.prop.meta" />
	  <None Include="data\Scripts\Character\CharacterController.cs.meta" />
