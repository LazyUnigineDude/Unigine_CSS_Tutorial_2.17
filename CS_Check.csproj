		<Compile Include="data\Scripts\AI\NavigationMaker.cs" />
		<Compile Include="data\Scripts\AI\PathMaker.cs" />
		<Compile Include="data\Scripts\AI\ShooterAI.cs" />
		<Compile Include="data\Scripts\Bullet_Health\Bullet.cs" />
		<Compile Include="data\Scripts\Bullet_Health\HealthBar.cs" />
		<Compile Include="data\Scripts\Camera\CameraController.cs" />
		<Compile Include="data\Scripts\Camera\InputController.cs" />
		<Compile Include="data\Scripts\Camera\ThirdPersonCamera.cs" />
		<Compile Include="data\Scripts\Character\Animation\AnimationController.cs" />
		<Compile Include="data\Scripts\Character\CharacterController.cs" />
		<Compile Include="data\Scripts\Character\GunHandler\GunHandler.cs" />
		<Compile Include="data\Scripts\Character\GunHandler\Interactor.cs" />
		<Compile Include="data\Scripts\Character\Inventory\InventoryController.cs" />
		<Compile Include="data\Scripts\Character\Inventory\InventoryGUI.cs" />
		<Compile Include="data\Scripts\Character\Inventory\InventoryMaker.cs" />
		<Compile Include="data\Scripts\Character\Physics\PhysicsController.cs" />
		<Compile Include="data\Scripts\Database\DatabaseController.cs" />
		<Compile Include="data\Scripts\GUI\HUDMaker.cs" />
		<Compile Include="data\Scripts\Menu\Menu.cs" />
		<Compile Include="data\Scripts\Sound\SoundManager.cs" />
		<Compile Include="data\ViewportClip.cs" />
	</ItemGroup>
	<ItemGroup>
		<Compile Include="source\AppEditorLogic.cs" />
		<Compile Include="source\AppSystemLogic.cs" />
		<Compile Include="source\AppWorldLogic.cs" />
		<Compile Include="source\main.cs" />
	</ItemGroup>
	<ItemGroup>
	  <None Include="data\Scripts\AI\AIDetector.cs.meta" />
	  <None Include="data\Scripts\AI\AIManager.cs.meta" />
	  <None Include="data\Scripts\AI\AIShootingLogic.cs.meta" />
	  <None Include="data\Scripts\AI\NavigationMaker.cs.meta" />
